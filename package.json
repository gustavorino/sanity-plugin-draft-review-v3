{
  "name": "sanity-plugin-draft-review-v3",
  "version": "0.0.1",
  "description": "This plugin allows content editors to quickly view, approve and reject all the documents in draft mode (not published).",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/gustavorino/sanity-plugin-draft-review-v3#readme",
  "bugs": {
    "url": "https://github.com/gustavorino/sanity-plugin-draft-review-v3/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/gustavorino/sanity-plugin-draft-review-v3.git"
  },
  "license": "MIT",
  "author": "Gustavo Bremm <gustavorino@gmail.com>",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "default": "./dist/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "run-s clean && plugin-kit verify-package --silent && pkg-utils build --strict && pkg-utils --strict",
    "clean": "rimraf dist",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "run-s build",
    "watch": "pkg-utils watch --strict"
  },
  "dependencies": {
    "@sanity/incompatible-plugin": "^1.0.4",
    "diff": "^5.1.0"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "^2.2.16",
    "@sanity/plugin-kit": "^3.1.7",
    "@types/diff": "^5.0.3",
    "@types/react": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prettier-plugin-packagejson": "^2.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rimraf": "^5.0.1",
    "sanity": "^3.11.3",
    "styled-components": "^5.3.11",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3"
  },
  "engines": {
    "node": ">=14"
  }
}
